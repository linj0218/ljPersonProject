	<script>
		var user_token = sessionStorage.getItem("logintoken");
		var user_id = sessionStorage.getItem("loginuid");
		var user_name = sessionStorage.getItem("loginuname");
		$(function () {

			//window.location.href = "login.html";
			$(".loginuser").html(user_name);
			if(sessionStorage.getItem("logintoken") == "" || sessionStorage.getItem("logintoken") == null){
				window.location.href = "login.html";
			}

			$("#navbarmenu > ul > li").each(function () {
				$(this).bind("click",menuclick);
			});
		});
		function menuclick() {
			$("#navbarmenu > ul > li").each(function () {
				$(this).removeClass("active");
			});
			$(this).addClass("active");
		}

		function fn_logout() {
			var para = {};
			para.ultoken = user_token;
			para.uid = parseInt(user_id);
			para.uluid = parseInt(user_id);
			para.uname = user_name;
			var jsondata = JSON.stringify(para);
			$.ajax({
				type: "GET",
				url: httpurl + "/userlogin/Userlogout",
				jsonp: 'callback',
				data: {"data": jsondata},
				dataType: "jsonp",
				cache: false,
				timeout: 5000,
				contentType: "application/x-www-form-urlencoded; charset=utf-8",
				success: function(data){
					sessionStorage.setItem("logintoken", null);
					sessionStorage.setItem("loginuid", null);
					sessionStorage.setItem("loginuname", null);
					window.location.href = "../app/login.html";
				}
			});
		}

	</script>
